<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Student bean -->
	<bean id="student" class="com.mycompany.spring.spring_framework.ioc.model.Student">
		<property name="name" value="Student Name"/>
	</bean>
	
	<!-- Lecturer bean -->
	<bean id="lecturer" class="com.mycompany.spring.spring_framework.ioc.model.Lecturer">
	</bean>
	
	<!-- Class bean -->
	<bean id="softwareArch" class="com.mycompany.spring.spring_framework.ioc.model.Class">
		<property name="name" value="Software Architecture"/>
	</bean>
	
	
	<bean id="webApp" class="com.mycompany.spring.spring_framework.ioc.model.Class"/>
	
	
	
	
</beans>
